{
    "id": "abantecart",
    "name": "Abantecart",
    "packaging_format": 1,
    "description": {
        "en": "Open source ecommerce platform to power online retail",
        "fr": "Open source ecommerce platform to power online retail"
    },
    "version": "1.3.2~ynh1",
    "url": "https://github.com/YunoHost-Apps/abantecart_ynh/issues",
    "license": "OSL-3.0",
    "maintainer": {
        "name": "",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": false,
    "services": [
        "nginx",
        "php7.3-cgi",
        "php7.3-fpm",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "admin",
                "type": "user",
                "help": {
                    "en": "Access admin page via https://<domain.tld>/<admin>/"
                }
            },
            {
                "name": "password",
                "type": "password",
                "optional": false
            },
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
